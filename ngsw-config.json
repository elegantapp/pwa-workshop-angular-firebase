{
  "index": "/index.html",
  "appData": {
    "version": "1.0.0",
    "changelog": "Initial release"
  },
  "assetGroups": [
    {
      "name": "appshell",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/styles.*.css",
          "/vendor.*.js",
          "/common.*.js",
          "/main.*.js",
          "/runtime.*.js",
          "/*polyfills.*.js"
        ]
      }
    },
    {
      "name": "modules",
      "installMode": "lazy",
      "resources": {
        "files": [
          "/*.js",
          "!/vendor.*.js",
          "!/common.*.js",
          "!/main.*.js",
          "!/runtime.*.js",
          "!/*polyfills.*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**/*.(svg|jpg|png|webp|gif)",
          "/svg/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "conf-data",
      "version": 1,
      "urls": ["/assets/data/data.json"],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 10,
        "maxAge": "3h",
        "timeout": "3s"
      }
    }
  ]
}
